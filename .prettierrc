{
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 150,
  "semi": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "vueIndentScriptAndStyle": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "trailingComma": "es5"
      }
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
